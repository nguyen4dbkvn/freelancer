
# -----------------------------------------------------------------------------
# --- GTS Enterprise
# --- Default custom config properties
# -----------------------------------------------------------------------------

# --------------------------------------------------------
# --- StartupInit: add additional fields to tables

# --- StartupInit
StartupInit.class=org.opengts.custom.gts.StartupInit

# --- StartupInit: Account
startupInit.Account.AddressFieldInfo=false
startupInit.Account.MapLegendFieldInfo=true
startupInit.Account.AccountManagerInfo=true

# --- StartupInit: User
startupInit.User.AddressFieldInfo=false

# --- StartupInit: Device
startupInit.Device.NotificationFieldInfo=true
#startupInit.Device.GeoCorridorFieldInfo=<hasRulePackage>
startupInit.Device.FixedLocationFieldInfo=false
startupInit.Device.LinkFieldInfo=false
#startupInit.Device.BorderCrossingFieldInfo=<SupportsBorderCrossing>
#startupInit.Device.MaintOdometerFieldInfo=true
startupInit.Device.WorkOrderInfo=false

# --- StartupInit: DeviceGroup
startupInit.DeviceGroup.WorkOrderInfo=false

# --- StartupInit: EventData
startupInit.EventData.AutoIncrementIndex=false
startupInit.EventData.CreationTimeMillisecond=false
startupInit.EventData.AddressFieldInfo=true
startupInit.EventData.GPSFieldInfo=true
startupInit.EventData.CustomFieldInfo=false
startupInit.EventData.CANBUSFieldInfo=false
startupInit.EventData.AtmosphereFieldInfo=false
startupInit.EventData.ThermoFieldInfo=false
startupInit.EventData.EndOfDaySummary=false
startupInit.EventData.ServingCellTowerData=false
startupInit.EventData.NeighborCellTowerData=false

# --- StartupInit: Geozone
startupInit.Geozone.PriorityFieldInfo=true
startupInit.Geozone.CorridorFieldInfo=false

# --------------------------------------------------------
# --- Table configuration

# --- EventData: 
EventData.keyedCreationTime=false

# --- Device: 
Device.futureDate.action=ignore
Device.futureDate.maximumSec=86400
#Device.invalidSpeed.action=ignore
#Device.invalidSpeed.maximumKPH=333
#Device.maximumOdometerKM=8333333

# --- Geozone: default radii
Geozone.dftRadius.pointRadius=3000
Geozone.dftRadius.polygon=50
Geozone.dftRadius.sweptPointRadius=1000

# --- RuleFactoryLite: enable email
RuleFactoryLite.emailEnabled=true

