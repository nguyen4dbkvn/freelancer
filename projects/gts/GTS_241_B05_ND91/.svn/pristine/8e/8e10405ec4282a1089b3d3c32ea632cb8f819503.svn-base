BODY {
     background-color: #5B5B67;
}

.navBar {
     background-color: #EFEFEF;
     border-bottom: 1px solid #555555;
     border-top: 1px solid #555555;
     font-size: 10pt;
     padding-bottom: 1px;
}

.navBarClear {
     font-size: 10pt;
     padding-bottom: 1px;
}

.formButton {
     margin: 0px;
     padding: 0px;
}

.textInput {

    background-color: #FFFFFF;
    border-color: #555588 #AAAABB #AAAABB #555588;
    border-radius: 5px 5px 5px 5px;
    border-style: solid;
    border-width: 1px;
  
    padding: 5px 5px 6px;

}
.textInput:focus { 
    background-color: #FFFFC9; 
}
.textReadOnly {

     background-color: #FFFFFF;
     border-radius: 5px 5px 5px 5px;
     border: 1px solid rgba(0, 0, 0, 0.25);
     box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.25);
     color: #000000 ! important;
     cursor: pointer;
     display: inline-block;
     font-size: 10px;
     font-weight: bold;
     line-height: 1;
     overflow: visible;
     padding: 5px 5px 6px;
     position: relative;
     text-decoration: none;
     width: auto;

}

.textReadOnly:focus {

    background-color: #E7E7E7;
    color: #111111;


}

.textOnClick {
     background-color: #FFFFFF;
     border-color: #444477 #9999AA #9999AA #444477;
     border-style: solid;
     border-width: 1px;
     cursor: pointer;
     border-radius: 5px 5px 5px 5px;
     padding: 5px 5px 6px;
}

.textAreaInput {
     background-color: #FFFFFF;
     border-color: #444477 #9999AA #9999AA #444477;
     border-style: solid;
     border-width: 1px;
     font-family: monospace;
     padding-left: 2px;
}

.textAreaReadOnly {
     background-color: #EFEFEF;
     border-color: #444477 #9999AA #9999AA #444477;
     border-style: solid;
     border-width: 1px;
     color: #555555;
     font-family: monospace;
     padding-left: 2px;
}

.textAreaReadOnly:focus {
     background-color: #E7E7E7;
     color: #111111;
}

.menuTitle {
     font-size: 12pt;
     font-weight: bold;
     height: 25px;
}

.menuInstructions {
     font-size: 10pt;
}

.menuDescription {
     font-size: 9pt;
     padding-top: 3px;
}

.contentTable {
     color: white;
     height: 550px;
     margin-top: 10px;
     max-height: 550px;
     width: 100%;
}

.contentCell {
     background-color: #F4F4FF;
     border: 0px solid #555555;
     padding: 5px;
     vertical-align: top;
}

.contentTopMenuCell {
     padding: 5px;
     text-align: left;
     vertical-align: top;
}

.contentMessage {
     font-style: oblique;
     margin-top: 5px;
     text-align: center;
}

.contentMapTable {
     height: 550px;
     margin-top: 10px;
     max-height: 550px;
     width: 844px;
}

.contentTrackMapCell {
     background-color: #0B1727;
     border: 2px solid #555555;
     font-size: 7pt;
     padding: 5px;
     vertical-align: top;
     width: 100%;
}

.contentMapTableFull {
     height: 99%;
     width: 100%;
}

.contentTrackMapCellFull {
     background-color: #0B1727;
     border: 0px solid #555555;
     font-size: 7pt;
     padding: 5px;
     vertical-align: top;
     width: 100%;
}

.messageResponseTable {
     margin-top: 50px;
}

.messageResponseCell {
     background-color: #FBFBFB;
     border-radius: 10px 10px 10px 10px;
     border: 1px solid #555555;
     color: black;
     padding: 30px;
     text-align: center;
}

.copyrightFooter {
     background-color: #EFEFEF;
     font-size: 7pt;
     padding-bottom: 2px;
     text-align: center;
}

.copyrightFooterClear {
     background-image: url("../images/footer.png");
     background-repeat: repeat-x;
     border: 1px solid #FFFFFF;
     color: #FFFFFF;
     font-size: 7pt;
     height: 40px;
     margin-bottom: 10px;
     margin-top: 10px;
     padding-bottom: 2px;
     text-align: center;
     width: 100%;
}

TBODY.scrollExp {
     max-height: 380px;
     overflow-x: hidden;
}

TBODY.scroll {
     height: 380px;
     max-height: 380px;
     overflow-x: hidden;
     overflow-y: scroll;
}

TBODY.noScroll {
    
}

