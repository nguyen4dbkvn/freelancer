.mGrid {
    border: 1px solid black;
    border-collapse: collapse;
    margin: 5px 0 10px;
    width: 100%;
}
.mGrid td {
    border: 1px solid #C1C1C1;
    padding: 5px;
}
.mGrid th {
    background: none repeat scroll 0 0 #00C0C0;
    border-left: 1px solid #525252;
    color: black;
    font-size: 0.9em;
    height: 30px;
    padding: 4px 2px;
}
.mGrid a {
    color: #3366FF;
}
.mGrid a:hover {
    color: Red;
}
.mGrid .alt {
    background-color: AliceBlue;
    color: black;
    font-weight: bold;
    height: 30px;
}
.mGrid .pgr {
    background: none repeat scroll 0 0 AliceBlue;
}
.mGrid .pgr table {
    margin: 5px 0;
}
.mGrid .pgr td {
    border-left: 1px solid #666666;
    border-width: 0 0 0 1px;
    color: #FFFFFF;
    font-weight: bold;
    line-height: 12px;
    padding: 0 6px;
}
.mGrid .pgr a {
    color: red;
    text-decoration: none;
}
.mGrid .pgr a:hover {
    color: #000000;
    text-decoration: none;
}
